@model Producto
@{
    ViewData["Title"] = "Crear Productos vista";
}
<h1>@ViewData["Title"]</h1>

<p>Esta es la vista para crear productos</p>

<form method="post">

    <div class="mb-3">
        <label>Nombre</label>
        <input asp-for="Nombre" class="form-control">
    </div>
    <div class="mb-3">
        <label>Descripcion</label>
        <input asp-for="Descripcion" class="form-control">
    </div>
    <div>
        <label>Cantidad</label>
        <input asp-for="Cantidad" class="form-control">
    </div>
    <div class="row">
        <div class="col-6 col-md-6">
            <button type="submit" class="btn btn-primary" asp-action="Create">Guardar</button>
        </div>
        <div class="col-6">
            <a asp-action="Index" class="btn btn-outline-secondary">Regresar</a>
        </div>
    </div>
   
</form>